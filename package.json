{
  "name": "freeclassway-api",
  "version": "0.1.0",
  "description": "RESTful API server for freeclassway",
  "main": "lib/index.js",
  "scripts": {
    "dev": "nodemon src/index.js --ignore locales/ --ignore lib/ --exec babel-node",
    "start": "NODE_ENV=production node lib/index.js",
    "migrate": "knex migrate:latest",
    "db": "knex migrate:latest && knex seed:run",
    "build": "babel src --out-dir lib && cp -R src/views lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Daniel Qin <taurencoder@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "0.8.7",
    "bignumber.js": "2.4.0",
    "bookshelf": "0.10.1",
    "camelize": "1.0.0",
    "cancan": "2.1.0",
    "config": "1.21.0",
    "hashids": "1.1.1",
    "joi": "9.0.4",
    "jwt-simple": "0.5.0",
    "knex": "0.12.1",
    "koa": "2.0.0",
    "koa-better-body": "3.0.2",
    "koa-bodyparser": "2.2.0",
    "koa-cors": "0.0.16",
    "koa-i18n": "2.0.1",
    "koa-locale": "1.2.0",
    "koa-passport": "2.2.2",
    "koa-router": "7.0.1",
    "koa-session2": "1.0.8",
    "koa-views": "5.0.2",
    "lodash": "4.15.0",
    "log4js": "0.6.38",
    "moment": "2.15.1",
    "node-uuid": "1.4.7",
    "pg": "6.1.0",
    "pingpp": "2.0.8",
    "qn": "1.3.0",
    "ramda": "^0.22.1",
    "seedrandom": "2.4.2",
    "shortid": "2.2.6",
    "sprintf": "0.1.5",
    "thenify": "3.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-register": "6.14.0",
    "eslint": "^3.6.0",
